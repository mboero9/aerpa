<?xml version="1.0" encoding="utf-8"?>
<Report Name="TramitesRecVsEspxReg" MarginTop="10" MarginBottom="10">
	<ReportHeader Height="0" />

	<PageHeader Height="40">
			<ReportGraphic PSFunction="logocorreo"
				X="0" Y="0" Width="40" Height="12" />
			<ReportLabel Text="AERPA" Align="RPT_ALIGN_CENTER"
				X="0" Y="7" Width="190" Height="5"
				FontSize="10" FontStyle="RPT_NORMAL" />
			<ReportSysField Type="RPT_SYSDATE" FontSize="10"
				FontStyle="RPT_NORMAL" Align="RPT_ALIGN_RIGHT"
				Format="d/m/Y\ H:i"
				X="140" Y="7" Width="50" Height="5" />
			<ReportLine X="0" Y="13" Width="190" Height="0"
				LineWidth=".05" />

			<ReportLabel Text="Trámites Recibidos vs. Esperados por Región"
				Align="RPT_ALIGN_CENTER"
				X="0" Y="15" Width="190" Height="10"
				FontSize="20" FontStyle="RPT_BOLDITALIC" />

			<ReportLabel Text="Región:"
				X="0" Y="25" Width="20" Height="5"
				FontStyle="RPT_NORMAL" FontSize="10" />
			<ReportLabel Text="RGI_CODIGO"
				X="20" Y="25" Width="12" Height="5"
				FontStyle="RPT_BOLD" FontSize="10" />
			<ReportLabel Text="RGI_DESCRIP"
				 X="35" Y="25" Width="150" Height="5"
				 FontStyle="RPT_BOLD" FontSize="10" />

			<ReportLabel Text="Fecha de Retiro desde:"
				 X="0" Y="30" Width="40" Height="5"
				 FontStyle="RPT_NORMAL" FontSize="10" />
			<ReportLabel Text="FECDESDE"
				 X="41" Y="30" Width="18" Height="5"
				 FontStyle="RPT_BOLD" FontSize="10" />
			<ReportLabel Text="al"
				 X="60" Y="30" Width="4" Height="5"
				 FontStyle="RPT_NORMAL" FontSize="10" />
			<ReportLabel Text="FECHASTA"
				 X="65" Y="30" Width="30" Height="5"
				 FontStyle="RPT_BOLD" FontSize="10" />
	</PageHeader>

	<MainGroup Field="TRA_FECHA_RETIRO+REG_COD_INT">
		<GroupHeader Height="20">
			<ReportLabel Text="Fecha de retiro:"
				 X="0" Y="0" Width="30" Height="5"
				 FontStyle="RPT_NORMAL" FontSize="10" />
			<ReportField FieldName="TRA_FECHA_RETIRO"
				 X="31" Y="0" Width="18" Height="5"
				 FontStyle="RPT_BOLD" FontSize="10" />

			<ReportLabel Text="Registro Destino:"
				 X="0" Y="5" Width="30" Height="5"
				 FontStyle="RPT_NORMAL" FontSize="10" />
			<ReportField FieldName="REG_COD_INT"
				 X="31" Y="5" Width="15" Height="5"
				 FontStyle="RPT_BOLD" FontSize="10" />
			<ReportLabel Text="-"
				 X="47" Y="5" Width="1" Height="5"
				 FontStyle="RPT_BOLD" FontSize="10" />				 
			<ReportField FieldName="DES_ORIGEN"
				 X="52" Y="5" Width="50" Height="5"
				 FontStyle="RPT_BOLD" FontSize="10" />

			<ReportLabel Text="Nro.de Trámite"
				X="0" Y="12" Width="39" Height="4"
				FontStyle="RPT_BOLD" FontSize="9" />
			<ReportLabel Text="Fecha de Retiro"
				X="28" Y="12" Width="39" Height="4"
				FontStyle="RPT_BOLD" FontSize="9" />
			<ReportLabel Text="Fecha de Entregado"
				X="55" Y="12" Width="39" Height="4"
				FontStyle="RPT_BOLD" FontSize="9" />
			<ReportLabel Text="Fecha de Devolución"
				X="88" Y="12" Width="39" Height="4"
				FontStyle="RPT_BOLD" FontSize="9" />
			<ReportLabel Text="Motivo"
				X="125" Y="12" Width="42" Height="4"
				FontStyle="RPT_BOLD" FontSize="9" />
			<ReportLine X="0" Y="17" Width="190" Height="0"
				LineWidth=".2" />
		</GroupHeader>

		<GroupFooter Height="7">
			<ReportLabel Text="Subtotal:"
				 X="0" Y="0" Width="30" Height="5"
				 FontStyle="RPT_NORMAL" FontSize="10" />
			<ReportCalcField X="31" Y="0" Width="40" Height="5"
				 FontStyle="RPT_BOLD" FontSize="9"
				 FieldName="TRA_DOMINIO" Operation="RPT_COUNT"
				 Format="%d" />
<!--			<ReportLine X="0" Y="5" Width="190" Height="0"
				LineWidth=".2" />-->
		</GroupFooter>
	</MainGroup>

	<Detail Height="4">
		<ReportField FieldName="TRA_DOMINIO"
			X="10" Y="0" Width="39" Height="4"
			FontStyle="RPT_NORMAL" FontSize="9" />
		<ReportField FieldName="TRA_FECHA_RETIRO"
			X="28" Y="0" Width="39" Height="4"
			FontStyle="RPT_NORMAL" FontSize="9" />
		<ReportField FieldName="TRA_FECHA_ENTREGA"
			X="55" Y="0" Width="39" Height="4"
			FontStyle="RPT_NORMAL" FontSize="9" />
		<ReportField FieldName="TRA_FECHA_DEVOLUCION"
			X="88" Y="0" Width="39" Height="4"
			FontStyle="RPT_NORMAL" FontSize="9" />
		<ReportField FieldName="MOT_DESCRIP"
			X="125" Y="0" Width="39" Height="4"
			FontStyle="RPT_NORMAL" FontSize="9" />

	</Detail>

	<PageFooter Height="10">
		<ReportLine X="0" Y="6" Width="190" Height="0"
			LineWidth=".05" />
		<ReportSysField Type="RPT_SYSPAGENO" FontSize="8"
			FontStyle="RPT_ITALIC" Align="RPT_ALIGN_CENTER"
			Format="Página %d"
			X="0" Y="7" Width="190" Height="3" />
	</PageFooter>

	<ReportFooter Height="7">
		<ReportLabel Text="Total General:"
			 X="0" Y="0" Width="25" Height="5"
			 FontStyle="RPT_NORMAL" FontSize="10" />
		<ReportCalcField X="26" Y="0" Width="10" Height="5"
			 FontStyle="RPT_BOLD" FontSize="9"
			 FieldName="TRA_FECHA_RETIRO" Operation="RPT_COUNT"
			 Format="%d" />				
		<ReportLine X="0" Y="6" Width="190" Height="0"
			LineWidth=".2" />
	</ReportFooter>		
</Report>