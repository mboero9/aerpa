<?xml version="1.0" encoding="utf-8"?>
<Report Name="conTramitesxReg" MarginTop="10" MarginBottom="10">
	<ReportHeader Height="15">
		<ReportLabel Text="Trámites por Región" Align="RPT_ALIGN_CENTER"
			X="0" Y="3" Width="190" Height="10"
			FontSize="20" FontStyle="RPT_BOLDITALIC" />
	</ReportHeader>

	<PageHeader Height="16">
		<ReportGraphic PSFunction="logocorreo"
			X="0" Y="0" Width="40" Height="12" />
		<ReportLabel Text="AERPA" Align="RPT_ALIGN_CENTER"
			X="0" Y="7" Width="190" Height="5"
			FontSize="10" FontStyle="RPT_NORMAL" />
		<ReportSysField Type="RPT_SYSDATE" FontSize="10"
			FontStyle="RPT_NORMAL" Align="RPT_ALIGN_RIGHT"
			Format="d/m/Y\ H:i"
			X="140" Y="7" Width="50" Height="5" />
		<ReportLine X="0" Y="13" Width="190" Height="0"
			LineWidth=".05" />
	</PageHeader>

	<MainGroup>
		<GroupHeader Height="15">
			<ReportLabel Text="Región:"
				X="0" Y="0" Width="20" Height="5"
				FontStyle="RPT_NORMAL" FontSize="10" />
			<ReportField FieldName="RGI_CODIGO"
				X="20" Y="0" Width="12" Height="5"
				FontStyle="RPT_BOLD" FontSize="10" />
			<ReportField FieldName="RGI_DESCRIP"
				 X="35" Y="0" Width="150" Height="5"
				 FontStyle="RPT_BOLD" FontSize="10" />

			<ReportLabel Text="Fecha de Retiro desde:"
				 X="0" Y="5" Width="40" Height="5"
				 FontStyle="RPT_NORMAL" FontSize="10" />
			<ReportField FieldName="FECDESDE"
				 X="41" Y="5" Width="18" Height="5"
				 FontStyle="RPT_BOLD" FontSize="10" />
			<ReportLabel Text="al"
				 X="60" Y="5" Width="4" Height="5"
				 FontStyle="RPT_NORMAL" FontSize="10" />
			<ReportField FieldName="FECHASTA"
				 X="65" Y="5" Width="30" Height="5"
				 FontStyle="RPT_BOLD" FontSize="10" />

			<ReportLabel Text="Código"
				X="0" Y="11" Width="20" Height="4"
				FontStyle="RPT_BOLD" FontSize="9" />
			<ReportLabel Text="Registro Automotor"
				X="20" Y="11" Width="39" Height="4"
				FontStyle="RPT_BOLD" FontSize="9" />
			<ReportLabel Text="Nro.de Trámite"
				X="75" Y="11" Width="39" Height="4"
				FontStyle="RPT_BOLD" FontSize="9" />
			<ReportLabel Text="Fecha de Retiro"
				X="108" Y="11" Width="39" Height="4"
				FontStyle="RPT_BOLD" FontSize="9" />
			<ReportLine X="0" Y="15" Width="190" Height="0"
				LineWidth=".2" />
		</GroupHeader>

		<GroupFooter Height="6">
			<ReportLine X="0" Y="0" Width="190" Height="0"
				LineWidth=".2" />
			<ReportLabel Text="Total de Trámites:"
				 X="0" Y="1" Width="25" Height="5"
				 FontStyle="RPT_NORMAL" FontSize="10" />
			<ReportCalcField X="26" Y="1" Width="10" Height="5"
				 FontStyle="RPT_BOLD" FontSize="9"
				 FieldName="TRA_DOMINIO" Operation="RPT_COUNT"
				 Format="%d" />				
			<ReportLine X="0" Y="6" Width="190" Height="0"
				LineWidth=".2" />
		</GroupFooter>
	</MainGroup>

	<Detail Height="4">
		<ReportField FieldName="REG_COD_INT"
			X="10" Y="0" Width="39" Height="4"
			FontStyle="RPT_NORMAL" FontSize="9" />
		<ReportField FieldName="DES_ORIGEN"
			X="20" Y="0" Width="39" Height="4"
			FontStyle="RPT_NORMAL" FontSize="9" />
		<ReportField FieldName="TRA_DOMINIO"
			X="75" Y="0" Width="39" Height="4"
			FontStyle="RPT_NORMAL" FontSize="9" />
		<ReportField FieldName="TRA_FECHA_RETIRO"
			X="108" Y="0" Width="39" Height="4"
			FontStyle="RPT_NORMAL" FontSize="9" />
	</Detail>

	<PageFooter Height="10">
		<ReportLine X="0" Y="6" Width="190" Height="0"
			LineWidth=".05" />
		<ReportSysField Type="RPT_SYSPAGENO" FontSize="8"
			FontStyle="RPT_ITALIC" Align="RPT_ALIGN_CENTER"
			Format="Página %d"
			X="0" Y="7" Width="190" Height="3" />
	</PageFooter>

	<ReportFooter Height="0" />
</Report>