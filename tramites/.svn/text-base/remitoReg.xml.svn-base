<?xml version="1.0" encoding="utf-8"?>
<Report Name="Remito por región" MarginTop="10" MarginBottom="10">
	<ReportHeader Height="15">
		<ReportLabel Text="Remito por región" Align="RPT_ALIGN_CENTER"
			X="0" Y="3" Width="190" Height="10"
			FontSize="20" FontStyle="RPT_BOLDITALIC" />
	</ReportHeader>

	<PageHeader Height="16">
		<ReportGraphic PSFunction="logocorreo"
			X="0" Y="0" Width="40" Height="12" />
		<ReportLabel Text="AERPA" Align="RPT_ALIGN_CENTER"
			X="0" Y="7" Width="190" Height="5"
			FontSize="10" FontStyle="RPT_NORMAL" />
		<ReportSysField Type="RPT_SYSDATE" FontSize="10"
			FontStyle="RPT_NORMAL" Align="RPT_ALIGN_RIGHT"
			Format="d/m/Y\ H:i"
			X="140" Y="7" Width="50" Height="5" />
		<ReportLine X="0" Y="13" Width="190" Height="0"
			LineWidth=".05" />
	</PageHeader>

	<MainGroup>
		<GroupHeader Height="20">
			<ReportLabel Text="Región destino:"
				X="0" Y="0" Width="40" Height="5"
				FontStyle="RPT_NORMAL" FontSize="10" />
			<ReportLabel Text="RGI_CODIGO"
				X="41" Y="0" Width="14" Height="5"
				FontStyle="RPT_BOLD" FontSize="10" />
			<ReportLabel Text="RGI_DESCRIP"
				 X="56" Y="0" Width="150" Height="5"
				 FontStyle="RPT_BOLD" FontSize="10" />

			<ReportLabel Text="Fecha de carga desde:"
				 X="0" Y="5" Width="40" Height="5"
				 FontStyle="RPT_NORMAL" FontSize="10" />
			<ReportLabel Text="CARGA_DESDE"
				 X="41" Y="5" Width="30" Height="5"
				 FontStyle="RPT_BOLD" FontSize="10" />

			<ReportLabel Text="Fecha de carga hasta:"
				 X="0" Y="10" Width="40" Height="5"
				 FontStyle="RPT_NORMAL" FontSize="10" />
			<ReportLabel Text="CARGA_HASTA"
				 X="41" Y="10" Width="30" Height="5"
				 FontStyle="RPT_BOLD" FontSize="10" />
		</GroupHeader>

		<GroupFooter Height="7">
			<ReportLine X="0" Y="0" Width="190" Height="0"
				LineWidth=".2" />
			<ReportLabel Text="Total región:"
				X="0" Y="2" Width="30" Height="5"
				FontStyle="RPT_NORMAL" FontSize="10" />
			<ReportCalcField FieldName="TRA_DOMINIO" Operation="RPT_COUNT"
				X="31" Y="2" Width="30" Height="5" Format="%d"
				FontStyle="RPT_BOLD" FontSize="10" />
		</GroupFooter>

		<SubGroup Field="REG_COD_INT">
			<GroupHeader Height="10">
				<ReportLabel Text="Registro destino:"
					X="0" Y="0" Width="40" Height="5"
					FontStyle="RPT_NORMAL" FontSize="10" />
				<ReportField FieldName="REG_COD_INT"
					X="41" Y="0" Width="14" Height="5"
					FontStyle="RPT_BOLD" FontSize="10" />
				<ReportField FieldName="REG_DESCRIP"
					X="56" Y="0" Width="130" Height="5"
					FontStyle="RPT_BOLD" FontSize="10" />

				<ReportLabel Text="Dominio"
					X="00" Y="5" Width="39" Height="4"
					FontStyle="RPT_BOLD" FontSize="9" />
				<ReportLine X="0" Y="10" Width="190" Height="0"
					LineWidth=".2" />
			</GroupHeader>

			<GroupFooter Height="6">
				<ReportLine X="0" Y="0" Width="190" Height="0"
					LineWidth=".2" />
				<ReportLabel Text="Total registro:"
					X="0" Y="0" Width="30" Height="5"
					FontStyle="RPT_NORMAL" FontSize="10" />
				<ReportCalcField FieldName="TRA_DOMINIO" Operation="RPT_COUNT"
					X="31" Y="0" Width="30" Height="5" Format="%d"
					FontStyle="RPT_BOLD" FontSize="10" />
			</GroupFooter>
		</SubGroup>
	</MainGroup>

	<Detail Height="4">
		<ReportField FieldName="TRA_DOMINIO"
			X="0" Y="0" Width="39" Height="4"
			FontStyle="RPT_NORMAL" FontSize="9" />
	</Detail>

	<PageFooter Height="10">
		<ReportLine X="0" Y="6" Width="190" Height="0"
			LineWidth=".05" />
		<ReportSysField Type="RPT_SYSPAGENO" FontSize="8"
			FontStyle="RPT_ITALIC" Align="RPT_ALIGN_CENTER"
			Format="Página %d"
			X="0" Y="7" Width="190" Height="3" />
	</PageFooter>

	<ReportFooter Height="0" />
</Report>