%
% Funciones para cambio de unidad
%
/inch {72 mul} def
/mm {inch 25.4 div} def

%
% Funciones para conversion de coordenadas
%
/px {} def
/py {neg ###height### add} def

%
% Impresion de texto
%
% imprime con clipping box partiendo de x y hacia abajo derecha w h
% align: 0 izq, 1 der, 2 cen
%
% stack: x y w h clip align text
%
/boxedtext {
  7 3 roll neg  % stack: clip align text x y w -h
  gsave
  % armar clipping box
  initclip
  7 -1 roll {
    4 copy        % stack: align text x y w h x y w h
    rectclip      % stack: align text x y w h
  } if

  .8 mul 3 -1 roll add  % stack: align text x w baseline
  5 1 roll  % stack: baseline align text x w
  2 index stringwidth pop   % stack: baseline align text x w strw
  5 -1 roll
  
  % stack: baseline text x w strw align

  % align left
  dup 0 eq { pop pop pop } if
  % align right
  dup 1 eq { pop neg add add } if
  % align center
  dup 2 eq { pop sub 2 div add } if

  3 -1 roll     % stack: text x baseline
  moveto show   % stack vacio
  grestore
} def

%
% Diccionarios de fonts encoding ISO-8859-1
%
/HelveticaLatin
  << /Helvetica findfont {} forall >>
  begin
    /Encoding ISOLatin1Encoding 256 array copy def currentdict
  end
